@model DMPTestProject.Models.RSS_Feed.ViewModels.AddRSSFeedViewModel
@{
}
<div class="container py-4">
    <form method="post">
        <div class="form-group row mb-3">
            <label for="inputUrl" class="col-sm-2 col-form-label">Url</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="inputUrl" placeholder="Url" asp-for="Url">
            </div>
        </div>
        <div class="form-group row mb-3">
            <label for="inputName" class="col-sm-2 col-form-label">Name</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="inputName" placeholder="Name" asp-for="Name">
            </div>
        </div>
        <div class="mb-3">
            <button type="submit" class="btn btn-primary">Submit</button>
        </div>
    </form>
</div>